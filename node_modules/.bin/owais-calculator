#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/usr/bin/emv" ]; then
  exec "$basedir/usr/bin/emv" node "$basedir/../owais-calculator/index.js" "$@"
else 
  exec usr/bin/emv node "$basedir/../owais-calculator/index.js" "$@"
fi
